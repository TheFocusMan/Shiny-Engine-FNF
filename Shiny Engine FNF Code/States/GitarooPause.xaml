<local:MusicBeatState x:Class="Shiny_Engine_FNF.Code.GitarooPause"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Shiny_Engine_FNF.Code"
     xmlns:visual="http://schemas.thefocusman.com/wpfgame/controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GitarooPause">

    <visual:Camera2D>
        <visual:Sprite x:Name="bg"/>
        <visual:Sprite x:Name="bf"/>
        <visual:Sprite x:Name="replayButton" MouseLeftButtonUp="replayButton_MouseLeftButtonUp"/>
        <visual:Sprite x:Name="cancelButton" MouseLeftButtonUp="cancelButton_MouseLeftButtonUp"/>
    </visual:Camera2D>
</local:MusicBeatState>
